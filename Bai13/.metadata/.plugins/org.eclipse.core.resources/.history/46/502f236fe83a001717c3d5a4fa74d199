package com.example.truong_hop_4;

public abstract class Employee {
	private String id;
	private String name;
	
	public String getId() {
		return id;
	}
	public void setId() {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName() {
		this.name = name;
	}
	public abstract double TinhLuong();
	@Override
	public String toString() {
		return this.id+"-"+this.name;
	}
}
